#include <bits/stdc++.h>
using namespace std;

int main() {
	priority_queue<int > maxh;
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++){
	    cin>>a[i];
	}
	for(int i=0;i<n;i++){
	    maxh.push(a[i]);
	}
	while(!maxh.empty()){
	    cout<<"This is the max_heap : "<<maxh.top()<<endl;
	    maxh.pop();
	}
	return 0;
}


INPUT : 6
100 2 34 56 290 1000

OUTPUT : 
This is the max_heap : 1000
This is the max_heap : 290
This is the max_heap : 100
This is the max_heap : 56
This is the max_heap : 34
This is the max_heap : 2
